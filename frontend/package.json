    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-vue": "9.27.0",
    "globals": "15.9.0",
    "husky": "9.1.5",
    "lint-staged": "15.2.9",
    "prettier": "3.3.3",
    "ts-node": "10.9.2",
    "typescript": "5.5.4",
    "typescript-eslint": "8.2.0",
    "vite-plugin-checker": "0.7.2",
    "vue-tsc": "2.0.29",
    "yaml": "2.5.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.**": [
      "eslint --fix",
      "prettier --ignore-unknown --write"
    ]
  },
  "browserslist": [
    "defaults"
